{
  "parser": "babel-eslint",
  "extends": ["standard", "prettier", "plugin:import/recommended"],
  "plugins": ["import", "node", "promise", "standard", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "arrowParens": "always",
        "semi": false,
        "singleQuote": true,
        "trailingComma": "all"
      }
    ]
  }
}
